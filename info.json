
{

    "name" : {

        "main" : "Outlook develop",
        "en"   : "Outlook develop",
        "es"   : "Outlook develop"

    },

    "version" : "0.0",
    "stage"   : "beta",
    "api"     : "a100d2",

    "language" : {

        "default" : "en-en",
        "list"    : [ "en-en" ]

    },

    "device" : {

        "desktop" : {

            "primary" : "main",
            "service" : ["js/service.js"],
            "common"  : {

                "css"    : [],
                "script" : [],
                "start"  : []

            },

            "view" : {

                "main" : {

                    "template" : "main.html",

                    "maxizable" : true,
                    "resizable" : true,

                    "width"  : {

                        "default" : 1000,
                        "min"     : 1000,
                        "max"     : 0

                    },

                    "height" : {

                        "default" : 480,
                        "min"     : 480,
                        "max"     : 0

                    },

                    "css"    : ["css/style.css"],
                    "script" : ["js/script.js"],
                    "start"  : ["js/start.js"]

                }

            }

        }

    },

    "permission" : {

        "service" : true,
        "integration" : {
            "outlook" : true
        },
        "fs" : {
            "create" : true,
            "modify" : true,
            "read"   : true
        }

    }

}
